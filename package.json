{
  "name": "solana-token-analyzer",
  "version": "1.0.0",
  "description": "Educational token analysis and dry-run simulation system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:rapid": "ts-node src/index.ts --mode=rapid",
    "dev:real": "ts-node src/index.ts --mode=real",
    "dev:unified": "ts-node src/index.ts --mode=unified",
    "dev:analysis": "ts-node src/index.ts --mode=analysis",
    "dev:enhanced": "ts-node src/enhanced-unified-system.ts educational unified",
    "dev:enhanced-analysis": "ts-node src/enhanced-unified-system.ts analysis unified",
    "dev:enhanced-monitoring": "ts-node src/enhanced-unified-system.ts monitoring unified",
    "start:rapid": "node dist/index.js --mode=rapid",
    "start:real": "node dist/index.js --mode=real",
    "start:unified": "node dist/index.js --mode=unified",
    "start:analysis": "node dist/index.js --mode=analysis",
    "start:enhanced": "node dist/enhanced-unified-system.js educational unified",
    "test": "jest",
    "test:enhanced": "ts-node test-enhanced-system.ts",
    "test:simplified": "ts-node test-simplified-system.ts",
    "dev:simplified": "ts-node src/simplified-unified-system.ts educational",
    "dev:simplified-analysis": "ts-node src/simplified-unified-system.ts analysis",
    "dev:simplified-monitoring": "ts-node src/simplified-unified-system.ts monitoring",
    "start:simplified": "node dist/simplified-unified-system.js educational",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.0",
    "@solana/spl-token": "^0.4.0",
    "ws": "^8.16.0",
    "dotenv": "^16.4.0",
    "winston": "^3.11.0",
    "express": "^4.18.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "axios": "^1.6.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": ["solana", "analysis", "educational", "monitoring"],
  "author": "Educational Research",
  "license": "MIT"
}