{
  "name": "solana-token-analyzer",
  "version": "1.0.0",
  "description": "Educational token analysis and dry-run simulation system",
  "main": "dist/consolidated-main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/consolidated-main.js",
    "dev": "ts-node src/consolidated-main.ts",
    "dev:rapid": "ts-node src/consolidated-main.ts --mode=rapid",
    "dev:real": "ts-node src/consolidated-main.ts --mode=real",
    "dev:unified": "ts-node src/consolidated-main.ts --mode=unified",
    "dev:analysis": "ts-node src/consolidated-main.ts --mode=analysis",
    "start:rapid": "node dist/consolidated-main.js --mode=rapid",
    "start:real": "node dist/consolidated-main.js --mode=real",
    "start:unified": "node dist/consolidated-main.js --mode=unified",
    "start:analysis": "node dist/consolidated-main.js --mode=analysis",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "legacy:dev": "ts-node src/index.ts",
    "legacy:dev:rapid": "ts-node src/index.ts --mode=rapid",
    "legacy:dev:real": "ts-node src/index.ts --mode=real",
    "legacy:dev:unified": "ts-node src/index.ts --mode=unified",
    "legacy:dev:analysis": "ts-node src/index.ts --mode=analysis",
    "legacy:start": "node dist/index.js",
    "legacy:start:rapid": "node dist/index.js --mode=rapid",
    "legacy:start:real": "node dist/index.js --mode=real",
    "legacy:start:unified": "node dist/index.js --mode=unified",
    "legacy:start:analysis": "node dist/index.js --mode=analysis"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.0",
    "@solana/spl-token": "^0.4.0",
    "ws": "^8.16.0",
    "dotenv": "^16.4.0",
    "winston": "^3.11.0",
    "express": "^4.18.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "axios": "^1.6.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@types/express": "^4.17.21",
    "typescript": "^5.3.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.0",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "keywords": ["solana", "analysis", "educational", "monitoring"],
  "author": "Educational Research",
  "license": "MIT"
}