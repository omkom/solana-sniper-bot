{
  "name": "solana-token-analyzer",
  "version": "1.0.0",
  "description": "Educational token analysis and dry-run simulation system",
  "main": "dist/consolidated-main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/consolidated-main.js --port=3000",
    "dev": "node startup-launcher.js --mode=unified --port=3000",
    "dev:rapid": "node startup-launcher.js --mode=rapid --port=3000",
    "dev:real": "node startup-launcher.js --mode=real --port=3000",
    "dev:unified": "node startup-launcher.js --mode=unified --port=3000",
    "dev:analysis": "node startup-launcher.js --mode=analysis --port=3000",
    "start:rapid": "node dist/consolidated-main.js --mode=rapid --port=3000",
    "start:real": "node dist/consolidated-main.js --mode=real --port=3000",
    "start:unified": "node dist/consolidated-main.js --mode=unified --port=3000",
    "start:analysis": "node dist/consolidated-main.js --mode=analysis --port=3000",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "diagnose": "node codespaces-diagnostic.js",
    "codespaces:setup": "npm install && chmod +x codespaces-diagnostic.js && echo 'Codespaces setup complete!'",
    "codespaces:test": "npm run diagnose && npm run dev",
    "auto-sync": "./scripts/start-auto-sync.sh",
    "stop-sync": "./scripts/stop-auto-sync.sh",
    "sync-status": "./scripts/status.sh"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "axios": "^1.6.0",
    "dotenv": "^16.4.0",
    "express": "^4.18.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.7.4",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chalk": "^5.4.1",
    "debug": "^4.4.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "solana",
    "analysis",
    "educational",
    "monitoring"
  ],
  "author": "Educational Research",
  "license": "MIT"
}
